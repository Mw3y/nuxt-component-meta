{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": ["node", "jest"],
    "allowJs": true
  }
}
